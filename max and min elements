//2.write a program to print maximum and minimum number from the array
#include<stdio.h>
int main()
{
	int a[5], i, min, max;
	printf("Enter array element\n");
	for(i=0; i<5; i++)
	{
		printf("a[%d]= ",i);
		scanf("%d", &a[i]);
	}
	min=a[0];
	for(i=1; i<5; i++)
	{
		if(a[i]<min)
		min=a[i];	
	}
	printf("The Minimum element is %d\n",min);
	max=a[0];
	for(i=1; i<5; i++)
	{
		if(a[i]>max)
		max=a[i];
	}
	printf("The maximum element is %d\n",max);
}
